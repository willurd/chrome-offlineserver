{
  "manifest_version": 2,
  "name": "HttpServer",
  "short_name": "HttpServer",
  "description": "Description for HttpServer",
  "version": "0.0.1.0",
  "content_security_policy": "unsafe-inline",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },

  "permissions": [
    "system.network"
  ],

  "sockets": {
    "tcpServer": {
      "listen": ["*:*"]
    }
  }
}
